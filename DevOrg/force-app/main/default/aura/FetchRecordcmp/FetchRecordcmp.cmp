<aura:component controller="FetchAccount" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global">
	<aura:attribute Name="MyAccount" type="Account[]" ></aura:attribute>
    
    <lightning:button label="Click to get" onclick="{!c.fetchAccount}"/>
    
    <aura:iteration items="{!v.MyAccount}" var="ac">
        <hr/>
        <table width="100%">
        	<tr>
              <td width="25%">
                {!ac.Id}
                </td>
            	<td width="25%">
                {!ac.Name}
                </td>
                <td width="25%">
                {!ac.Rating}
                </td>
                <td width="25%">
                {!ac.Website}
                </td>
            </tr>
        </table>
        
        
        
    </aura:iteration>
</aura:component>